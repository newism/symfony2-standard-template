{% extends 'ClubEvoApiBundle::layout.html.twig' %}

{% block content -%}
    <h1>Roadside Assist Employee</h1>

    <table class="record_properties">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ entity.id }}</td>
            </tr>
            <tr>
                <th>Role</th>
                <td>{{ entity.role }}</td>
